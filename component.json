            "description": "Updates an Email Template.",
            "fields": {
                "verbose": {
                    "viewClass": "CheckBoxView",
                    "label": "Debug this step (log more data)"
                }
            },
            "metadata": {
                "in": "./lib/schemas/EmailTemplate_CreateOrUpdate.in.json",
                "out": {
                    "type": "object"
                }
            }
        }
    }
}